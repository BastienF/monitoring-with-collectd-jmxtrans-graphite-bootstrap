{
    "queries": [
        {
            "objectName": "java.lang:type=GarbageCollector,name=*",
            "resultAlias": "jvm.gc.%name%",
            "attributes": [
                "CollectionCount",
                "CollectionTime"
            ]
        }
    ],
    "outputWriters": [
        {
            "@class": "org.jmxexporter.output.Slf4jWriter"
        },
        {
            "@class": "org.jmxexporter.output.GraphiteWriter",
            "settings": {
                "host": "${graphite.host:localhost}",
                "port": "${graphite.port:2003}"
            }
        }
    ],
    "queryIntervalInSeconds": 3,
    "numQueryThreads": 1,
    "exportIntervalInSeconds": 5
}